	file = {Kelly et al. - 2015 - Impacts of Vehicle Weight Reduction via Material S.pdf:C\:\\Users\\hhowa\\Zotero\\storage\\N6XXI52L\\Kelly et al. - 2015 - Impacts of Vehicle Weight Reduction via Material S.pdf:application/pdf},
