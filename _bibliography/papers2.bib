@article{nimbalkar_life_nodate,
	title = {Life {Cycle} {Energy} {Assessment} {Methodology} and {Additive} {Manufacturing} {Energy} {Impacts} {Assessment} {Tool}},
	abstract = {Additive Manufacturing (AM) processes enable the manufacture of optimized parts with minimal material requirements, and the products manufactured using the AM processes have the potential to reduce energy over a product’s entire lifespan. Typically, engineering analyses of AM focus on the manufacturing phase, but when assessing the life cycle energy of an additively manufactured product, it is vital to consider the material, manufacture, freight and distribution, use, and the disposal phases. Although AM processes may use significantly more energy than conventional processes per unit mass of material processed, parts with optimized shapes and geometric features can yield significant weight savings. For example, in aircraft parts, the primary environmental and energy efficiency benefit from lighter, optimized parts is the fuel savings realized during the use phase. Hence, there is a need for a consistent methodology to calculate life cycle energy consumption and savings attributable to parts and products additively manufactured. An AM methodology and associated tool were developed to enable estimates of life cycle energy consumption and savings.},
	language = {en},
	author = {Nimbalkar, Sachin and Cox, Daryl and Visconti, Kelly and Cresko, Joseph},
