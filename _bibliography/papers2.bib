	file = {Schöggl et al. - 2017 - Improving sustainability performance in early phas.pdf:C\:\\Users\\hhowa\\Zotero\\storage\\D7NHJZ3W\\Schöggl et al. - 2017 - Improving sustainability performance in early phas.pdf:application/pdf},
}

