	file = {Nuss and Eckelman - 2014 - Life Cycle Assessment of Metals A Scientific Synt.pdf:C\:\\Users\\hhowa\\Zotero\\storage\\FLRVY4BB\\Nuss and Eckelman - 2014 - Life Cycle Assessment of Metals A Scientific Synt.pdf:application/pdf},
